-- ðŸ”§ PERFORMANCE SETUP
local startLoad = tick()
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local UserInputService = game:GetService("UserInputService")

-- âš¡ FPS COUNTER
local FPS = 0
local lastTime = tick()
local frameCount = 0

RunService.RenderStepped:Connect(function()
    frameCount = frameCount + 1
    local currentTime = tick()
    if currentTime - lastTime >= 1 then
        FPS = frameCount
        frameCount = 0
        lastTime = currentTime
    end
end)

-- ðŸŽµ SOUND SYSTEM
local SoundSystem = {
    Play = function()
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://9113472375"
        sound.Volume = 0.4
        sound.Parent = Workspace
        sound:Play()
        game:GetService("Debris"):AddItem(sound, 1)
    end
}

-- ðŸ’° MONEY HACK
local MoneyHack = {
    Active = false,
    
    Execute = function()
        pcall(function()
            -- Hack leaderstats
            local stats = Player:FindFirstChild("leaderstats")
            if stats then
                for _, v in pairs(stats:GetChildren()) do
                    if v:IsA("IntValue") or v:IsA("NumberValue") then
                        local name = v.Name:lower()
                        if name:find("cash") or name:find("money") or name:find("coin") then
                            v.Value = 9999999
                        end
                    end
                end
            end
            
            -- Hack brainot count
            for _, v in pairs(Player:GetChildren()) do
                if v:IsA("IntValue") or v:IsA("NumberValue") then
                    if v.Name:lower():find("brainot") then
                        v.Value = 99999
                    end
                end
            end
        end)
    end
}

-- ðŸ§  BRAINOT DUPE
local DupeSystem = {
    Execute = function()
        pcall(function()
            local brainots = {}
            
            -- Collect all brainots
            for _, obj in pairs(Workspace:GetDescendants()) do
                if obj:IsA("BasePart") and obj.Name:lower():find("brainot") then
                    table.insert(brainots, obj)
                end
            end
            
            -- Duplicate each
            for _, brainot in pairs(brainots) do
                for i = 1, 3 do
                    local clone = brainot:Clone()
                    clone.Parent = Workspace
                    clone.Position = brainot.Position + Vector3.new(
                        math.random(-2, 2),
                        math.random(0, 1),
                        math.random(-2, 2)
                    )
                end
            end
        end)
    end
}

-- ðŸ¤– AUTO FARM
local FarmSystem = {
    Active = false,
    Distance = 100,
    
    Execute = function()
        pcall(function()
            local char = Player.Character
            if char then
                local root = char:FindFirstChild("HumanoidRootPart")
                if root then
                    for _, obj in pairs(Workspace:GetDescendants()) do
                        if obj:IsA("BasePart") then
                            local name = obj.Name:lower()
                            if name:find("brainot") or name:find("orb") or 
                               name:find("coin") or name:find("cash") then
                                if (root.Position - obj.Position).Magnitude <= FarmSystem.Distance then
                                    firetouchinterest(root, obj, 0)
                                    firetouchinterest(root, obj, 1)
                                end
                            end
                        end
                    end
                end
            end
        end)
    end
}

-- âœˆï¸ FLY SYSTEM
local FlySystem = {
    Enabled = false,
    Speed = 50,
    BodyVelocity = nil,
    
    Toggle = function(state)
        FlySystem.Enabled = state
        if FlySystem.Enabled then
            local root = Player.Character:FindFirstChild("HumanoidRootPart")
            if root then
                FlySystem.BodyVelocity = Instance.new("BodyVelocity")
                FlySystem.BodyVelocity.Parent = root
                FlySystem.BodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
                FlySystem.BodyVelocity.Velocity = Vector3.new(0, 0, 0)
            end
        elseif FlySystem.BodyVelocity then
            FlySystem.BodyVelocity:Destroy()
            FlySystem.BodyVelocity = nil
        end
    end
}

-- ðŸ‘ï¸ ESP SYSTEM
local ESPSystem = {
    Enabled = false,
    Highlights = {},
    
    Toggle = function(state)
        ESPSystem.Enabled = state
        if ESPSystem.Enabled then
            for _, obj in pairs(Workspace:GetDescendants()) do
                if obj:IsA("BasePart") and obj.Name:lower():find("brainot") then
                    if not ESPSystem.Highlights[obj] then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "BrainotESP"
                        highlight.FillColor = Color3.fromRGB(255, 50, 150)
                        highlight.OutlineColor = Color3.fromRGB(200, 30, 120)
                        highlight.FillTransparency = 0.3
                        highlight.OutlineTransparency = 0
                        highlight.Parent = obj
                        ESPSystem.Highlights[obj] = highlight
                    end
                end
            end
        else
            for obj, highlight in pairs(ESPSystem.Highlights) do
                if highlight then
                    highlight:Destroy()
                end
            end
            ESPSystem.Highlights = {}
        end
    end
}

-- ðŸš€ TELEPORT SYSTEM
local TeleportSystem = {
    Areas = {
        ["ðŸŒŒ Celestial Realm"] = CFrame.new(150, 100, -200),
        ["ðŸ”® Secret Chamber"] = CFrame.new(-300, 50, 150),
        ["ðŸ’° Money Zone"] = CFrame.new(500, 30, 400),
        ["âš”ï¸ Battle Arena"] = CFrame.new(0, 10, 0),
        ["ðŸ† VIP Area"] = CFrame.new(1000, 200, 1000),
        ["ðŸš€ Sky Platform"] = CFrame.new(0, 500, 0),
        ["ðŸŒŠ Wave Center"] = CFrame.new(0, 5, 0)
    },
    
    Teleport = function(areaName)
        pcall(function()
            local cf = TeleportSystem.Areas[areaName]
            if cf then
                local char = Player.Character
                if char then
                    local root = char:FindFirstChild("HumanoidRootPart")
                    if root then
                        root.CFrame = cf
                    end
                end
            end
        end)
    end
}

