-- FPS LIMIT
GraphicsSection:CreateSlider({
    Name = "FPS Limit",
    Range = {30, 240},
    Increment = 10,
    Suffix = "FPS",
    CurrentValue = 60,
    Flag = "FPSLimit",
    Callback = function(value)
        SoundSystem.Play()
        if value == 240 then
            settings().Rendering.FramerateMode = Enum.FramerateMode.Unlocked
        else
            settings().Rendering.FramerateMode = Enum.FramerateMode.Capped
            settings().Rendering.FramerateMax = value
        end
    end
})

-- GRAPHICS QUALITY
GraphicsSection:CreateDropdown({
    Name = "Graphics Quality",
    Options = {"Low", "Medium", "High", "Ultra"},
    CurrentOption = "Medium",
    Flag = "GraphicsQuality",
    Callback = function(option)
        SoundSystem.Play()
        if option == "Low" then
            settings().Rendering.QualityLevel = 1
        elseif option == "Medium" then
            settings().Rendering.QualityLevel = 5
        elseif option == "High" then
            settings().Rendering.QualityLevel = 8
        else
            settings().Rendering.QualityLevel = 21
        end
    end
})

-- SMOOTH GRAPHICS TOGGLE
GraphicsSection:CreateToggle({
    Name = "ðŸ”„ Smooth Graphics",
    CurrentValue = false,
    Flag = "SmoothGraphics",
    Callback = function(value)
        SoundSystem.Play()
        if value then
            settings().Rendering.EnableFRM = true
            settings().Rendering.MeshPartDetailLevel = Enum.MeshPartDetailLevel.Level20
        else
            settings().Rendering.EnableFRM = false
        end
    end
})

