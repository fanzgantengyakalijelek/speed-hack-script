    },
    KeySystem = false,
    KeySettings = {
        Title = "FanzXyzz System",
        Subtitle = "Enter Key",
        Note = "Join @fanzxyzz_aydoll for key",
        FileName = "FanzKey",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"FanzXyzzVIP2024"}
    }
})

-- MAIN TAB
local MainTab = Window:CreateTab("üè† Main", 4483362458)
local TeleportSection = MainTab:CreateSection("üìç Teleport Areas")
local MovementSection = MainTab:CreateSection("‚ö° Movement")

-- AREAS DATA
local Areas = {
    ["Spawn Zone"] = CFrame.new(0, 5, 0),
    ["Celestial Area"] = CFrame.new(150, 100, -200),
    ["Secret Chamber"] = CFrame.new(-300, 50, 150),
    ["Brainot Farm"] = CFrame.new(80, 30, 300),
    ["VIP Zone"] = CFrame.new(500, 200, 500),
    ["Hidden Temple"] = CFrame.new(-150, 25, -300),
    ["Money Area"] = CFrame.new(400, 40, 200)
}

-- TELEPORT DROPDOWN
local SelectedArea = "Spawn Zone"
local AreaDropdown = TeleportSection:CreateDropdown({
    Name = "Select Area",
    Options = {"Spawn Zone", "Celestial Area", "Secret Chamber", "Brainot Farm", "VIP Zone", "Hidden Temple", "Money Area"},
    CurrentOption = "Spawn Zone",
    Flag = "AreaDropdown",
    Callback = function(Value)
        SelectedArea = Value
    end
})

-- TELEPORT BUTTON
TeleportSection:CreateButton({
    Name = "üöÄ Teleport Now",
    Callback = function()
        local plr = game.Players.LocalPlayer
        if plr.Character then
            plr.Character.HumanoidRootPart.CFrame = Areas[SelectedArea]
            Rayfield:Notify({
                Title = "Teleport Success",
                Content = "Teleported to " .. SelectedArea,
                Duration = 3,
                Image = 4483362458
            })
        end
    end
})

-- WALK SPEED
MovementSection:CreateSlider({
    Name = "Walk Speed",
    Range = {16, 500},
    Increment = 5,
    Suffix = "studs",
    CurrentValue = 50,
    Flag = "WalkSpeed",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

-- JUMP POWER
MovementSection:CreateSlider({
    Name = "Jump Power",
    Range = {50, 500},
    Increment = 10,
    Suffix = "power",
    CurrentValue = 50,
    Flag = "JumpPower",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
    end
})

-- FLY SYSTEM
local Flying = false
local FlySpeed = 50
local FlyBodyVel

MovementSection:CreateToggle({
    Name = "Fly Mode",
    CurrentValue = false,
    Flag = "FlyToggle",
    Callback = function(Value)
        Flying = Value
        if Flying then
            local root = game.Players.LocalPlayer.Character.HumanoidRootPart
            FlyBodyVel = Instance.new("BodyVelocity", root)
            FlyBodyVel.MaxForce = Vector3.new(10000, 10000, 10000)
            FlyBodyVel.Velocity = Vector3.new(0, 0, 0)
            
            Rayfield:Notify({
                Title = "Fly Activated",
                Content = "Use W/A/S/D + Space/Shift to fly",
                Duration = 3,
                Image = 4483362458
            })
        else
            if FlyBodyVel then FlyBodyVel:Destroy() end
        end
    end
})

-- FLY SPEED
MovementSection:CreateSlider({
    Name = "Fly Speed",
    Range = {10, 200},
    Increment = 5,
    Suffix = "speed",
    CurrentValue = 50,
    Flag = "FlySpeed",
    Callback = function(Value)
        FlySpeed = Value
    end
})

-- NOCLIP
local Noclip = false
MovementSection:CreateToggle({
    Name = "Noclip",
    CurrentValue = false,
    Flag = "NoclipToggle",
    Callback = function(Value)
        Noclip = Value
    end
})

-- INFINITE JUMP
MovementSection:CreateToggle({
    Name = "Infinite Jump",
    CurrentValue = false,
    Flag = "InfiniteJump",
    Callback = function(Value)
        if Value then
            game:GetService("UserInputService").JumpRequest:Connect(function()
                game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
            end)
        end
    end
})

-- AUTO FARM TAB
local FarmTab = Window:CreateTab("ü§ñ Auto Farm", 4483362458)
local AutoSection = FarmTab:CreateSection("üí∞ Auto Collection")

-- INFINITY MONEY
AutoSection:CreateToggle({
    Name = "üí∞ Infinity Money Hack",
    CurrentValue = false,
    Flag = "InfMoney",
    Callback = function(Value)
        if Value then
            spawn(function()
                while wait(0.5) do
                    pcall(function()
                        local leaderstats = game.Players.LocalPlayer:FindFirstChild("leaderstats")
                        if leaderstats then
                            for _, stat in pairs(leaderstats:GetChildren()) do
                                if stat:IsA("IntValue") or stat:IsA("NumberValue") then
                                    if stat.Name:lower():find("cash") or stat.Name:lower():find("money") or stat.Name:lower():find("coin") then
                                        stat.Value = 999999
                                    end
                                end
                            end
                        end
                        
                        -- BRAINOT HACK
                        for _, obj in pairs(game.Players.LocalPlayer:GetChildren()) do
                            if obj:IsA("IntValue") or obj:IsA("NumberValue") then
                                if obj.Name:lower():find("brainot") then
                                    obj.Value = 99999
                                end
                            end
                        end
                    end)
                end
            end)
        end
    end
})

-- AUTO COLLECT BRAINOT
local AutoBrainot = false
AutoSection:CreateToggle({
    Name = "üß† Auto Collect Brainot",
    CurrentValue = false,
    Flag = "AutoBrainot",
    Callback = function(Value)
        AutoBrainot = Value
        
        spawn(function()
            while AutoBrainot do
                wait(0.2)
                local plr = game.Players.LocalPlayer
                if plr.Character then
                    local root = plr.Character.HumanoidRootPart
                    
                    -- COLLECT BRAINOTS
                    for _, obj in pairs(game.Workspace:GetDescendants()) do
                        if not AutoBrainot then break end
                        
                        if obj:IsA("Part") and (obj.Name:lower():find("brainot") or obj.Name:lower():find("orb")) then
                            if (root.Position - obj.Position).Magnitude < 100 then
                                firetouchinterest(root, obj, 0)
                                firetouchinterest(root, obj, 1)
                            end
                        end
                    end
                end
            end
        end)
    end
})

-- AUTO COLLECT MONEY
local AutoMoney = false
AutoSection:CreateToggle({
    Name = "üíµ Auto Collect Money",
    CurrentValue = false,
    Flag = "AutoMoney",
    Callback = function(Value)
        AutoMoney = Value
        
        spawn(function()
            while AutoMoney do
                wait(0.3)
                local plr = game.Players.LocalPlayer
                if plr.Character then
                    local root = plr.Character.HumanoidRootPart
                    
                    -- COLLECT MONEY
                    for _, obj in pairs(game.Workspace:GetDescendants()) do
                        if not AutoMoney then break end
                        
                        if obj:IsA("Part") and (obj.Name:lower():find("coin") or obj.Name:lower():find("cash") or obj.Name:lower():find("money")) then
                            if (root.Position - obj.Position).Magnitude < 100 then
                                firetouchinterest(root, obj, 0)
                                firetouchinterest(root, obj, 1)
                            end
                        end
                    end
                end
            end
        end)
    end
})

-- AUTO WIN WAVE
AutoSection:CreateToggle({
    Name = "üèÜ Auto Win Wave",
    CurrentValue = false,
    Flag = "AutoWin",
    Callback = function(Value)
        if Value then
            spawn(function()
                while wait(0.5) do
                    pcall(function()
                        -- KILL ALL ENEMIES
                        for _, enemy in pairs(game.Workspace:GetChildren()) do
                            if enemy:FindFirstChild("Humanoid") and (enemy.Name:lower():find("enemy") or enemy.Name:lower():find("mob")) then
                                enemy.Humanoid.Health = 0
                            end
                        end
                    end)
                end
            end)
        end
    end
})

-- GOD MODE
AutoSection:CreateToggle({
    Name = "üõ°Ô∏è God Mode",
    CurrentValue = false,
    Flag = "GodMode",
    Callback = function(Value)
        if Value then
            game.Players.LocalPlayer.Character.Humanoid.MaxHealth = math.huge
            game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
        else
            game.Players.LocalPlayer.Character.Humanoid.MaxHealth = 100
            game.Players.LocalPlayer.Character.Humanoid.Health = 100
        end
    end
})

-- VISUAL TAB
local VisualTab = Window:CreateTab("üé® Visual", 4483362458)
local VisualSection = VisualTab:CreateSection("üëÅÔ∏è Visual Effects")

-- ESP BRAINOT
local ESPEnabled = false
VisualSection:CreateToggle({
    Name = "üß† Brainot ESP",
    CurrentValue = false,
    Flag = "ESP",
    Callback = function(Value)
        ESPEnabled = Value
        
        if ESPEnabled then
            -- CREATE HIGHLIGHTS
            for _, obj in pairs(game.Workspace:GetDescendants()) do
                if obj:IsA("Part") and obj.Name:lower():find("brainot") then
                    local highlight = Instance.new("Highlight")
                    highlight.Name = "RayfieldESP"
                    highlight.FillColor = Color3.fromRGB(0, 255, 255)
                    highlight.OutlineColor = Color3.fromRGB(0, 200, 255)
                    highlight.FillTransparency = 0.3
                    highlight.Parent = obj
                end
            end
        else
            -- REMOVE HIGHLIGHTS
            for _, obj in pairs(game.Workspace:GetDescendants()) do
                if obj:FindFirstChild("RayfieldESP") then
                    obj.RayfieldESP:Destroy()
                end
            end
        end
    end
})

-- TRACER
VisualSection:CreateToggle({
    Name = "üéØ Tracer to Brainot",
    CurrentValue = false,
    Flag = "Tracer",
    Callback = function(Value)
        -- Tracer implementation
    end
})

-- PLAYER VISUALS
VisualSection:CreateToggle({
    Name = "‚ú® Player Glow",
    CurrentValue = false,
    Flag = "PlayerGlow",
    Callback = function(Value)
        if Value then
            local highlight = Instance.new("Highlight")
            highlight.Name = "PlayerGlow"
            highlight.FillColor = Color3.fromRGB(255, 0, 255)
            highlight.OutlineColor = Color3.fromRGB(200, 0, 200)
            highlight.FillTransparency = 0.5
            highlight.Parent = game.Players.LocalPlayer.Character
        else
            if game.Players.LocalPlayer.Character:FindFirstChild("PlayerGlow") then
                game.Players.LocalPlayer.Character.PlayerGlow:Destroy()
            end
        end
    end
})

